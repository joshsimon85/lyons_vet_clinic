dl.generic {
  margin-right: 7rem;
  dt {
    font-size: 1.25rem;
    font-weight: 700;
    margin-bottom: .5rem;
  }
  dd {
    font-size: 1.1rem;
  }
}

dl.generic:nth-of-type(3) {
  margin-right: 0;
}

section {
  ol {
    font-size: 1.1rem;
    li:nth-of-type(2n+1) {
      font-weight: 700;
    }
    li:nth-of-type(2n+2) {
      margin-left: .5rem;
    }
  }
}
