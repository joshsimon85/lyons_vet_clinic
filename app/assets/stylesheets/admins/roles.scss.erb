dl:first-of-type {
  dt {
    font-size: 1.25rem;
    font-weight: 700;
    margin-bottom: .5rem;
  }
  dd {
    font-size: 1.1rem;
  }
}

section {
  ol {
    font-size: 1.1rem;
    li:nth-of-type(2n+1) {
      font-weight: 700;
    }
    li:nth-of-type(2n+2) {
      margin-left: .5rem;
    }
  }
}
